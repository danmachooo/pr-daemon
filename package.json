{
  "name": "pr-daemon",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "postinstall": "npx prisma generate",
    "dev:api": "tsx watch src/server.ts",
    "dev:worker": "tsx watch src/worker.ts",
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:worker\"",
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "start": "node dist/server.js",
    "start:worker": "tsx src/worker.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc -p tsconfig.json",
    "test": "echo \"No tests yet.\""
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.3.0",
    "axios": "^1.13.2",
    "better-auth": "^1.4.12",
    "cors": "^2.8.5",
    "express": "^5.2.1",
    "node-cron": "^4.2.1",
    "pg": "^8.16.3",
    "winston": "^3.19.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@faker-js/faker": "^10.2.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "jsdom": "^25.0.1",
    "prisma": "^7.3.0",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0"
  }
}
